{
    "name": "ayzamodul/addchat",
    "version": "1.0.2",
    "description": "Live chat widget for Laravel that also includes multi-user chat, group permissions, customer support chat & more.",
  
    "license": "MIT",
    
 
    "authors": [
        {
            "name": "Ahsen Tekdemir",
            "email": "info@ahsentekdemir.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.5|~6.0"
    },
    "require-dev": {
        "laravel/framework": "~5.5|~6.0"
    },
    "autoload": {
        "psr-4": {
            "Ayzamodul\\Addchat\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable" : true,
    "extra": {
        "laravel": {
            "providers": [
                "Ayzamodul\\Addchat\\AddchatServiceProvider"
            ]
        }
    }
}
